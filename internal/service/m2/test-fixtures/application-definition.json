{
  "definition": {
    "listeners": [
      {
        "port": 8196,
        "type": "http"
      }
    ],
    "ba-application": {
      "app-location": "$${s3-source}/PlanetsDemo-${version}.zip"
    }
  },
  "source-locations": [
    {
      "source-id": "s3-source",
      "source-type": "s3",
      "properties": {
        "s3-bucket": "${s3_bucket}",
        "s3-key-prefix": "${version}"
      }
    }
  ],
  "template-version": "2.0"
}